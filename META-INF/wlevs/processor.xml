<?xml version="1.0" encoding="UTF-8"?>
<n1:config xmlns:n1="http://www.bea.com/ns/wlevs/config/application">
  <processor>
    <name>helloworldProcessor</name>
    <rules>
      <query id="helloworldRule">
        <![CDATA[ select * from helloworldInputChannel ]]>
      </query>
    </rules>
  </processor>
 <rest-adapter>
   <name>restJsonOutbound</name>
   <url>http://localhost:8080/RESTfulExample/rest/postService</url>
 </rest-adapter>
 <json-mapper>
  <name>jsonMapperBean</name>
  <event-type-name>RestEvent</event-type-name>
  <media-type>application/json</media-type>
</json-mapper> 
 
</n1:config>
